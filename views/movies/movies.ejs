<h1>Movies</h1>
<a href="/movies/new" class="btn btn-primary btn-lg">Add</a>
<% movies.forEach(movie => { %>


    <a href="/movies/<%= movie._id %>">
     <%= movie.name %>
   </a>


    <div class="single-item">
    <div>
    <a href="/movies/<%= movie._id %>"> <img src="<%= movie.image %>"> </a>
    </div>
    </div>

  
  <% }); %>
